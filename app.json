{
  "expo": {
    "name": "DevStore",
    "slug": "devstore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0693E3"
    },
    "scheme": "devstore",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pagaleve.devstore"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0693E3",
      "translucent": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0693E3"
      },
      "package": "com.pagaleve.devstore",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "devstore"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
